[user]

[core]
    editor = vim
[color]
    ui = true
[alias]
    a     = add
    b     = branch
    c     = checkout
    cm    = checkout master
    co    = commit
    di    = diff
    din   = diff --name-only
    diw   = diff --color-words
    f     = fetch
    fo    = fetch origin
    graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
    list  = stash list
    lo    = log --oneline
    m     = merge
    mo    = merge origin/master
    p     = push
    pop   = stash pop
    prune = remote prune upstream
    re    = remote
    st    = status
    tmp   = stash save
