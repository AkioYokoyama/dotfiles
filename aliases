# command
alias l="ls -lA"
alias ls="ls -FG"
alias ll="ls -alFG"

# git
alias ggn="git grep -n "
alias ggni="git grep -n -i "
alias tmp="git add . && git commit -m 'tmp commit'"

# shortcut
alias memo="$HOME/tools/shortcut/rust_command/note/target/release/note"
alias brow="bash $HOME/tools/shortcut/wrapper_shell/open_url.sh"
alias pass="bash $HOME/tools/shortcut/wrapper_shell/password_manager.sh"
alias sound="afplay /System/Library/Sounds/Submarine.aiff"
alias reload="source $HOME/.zshrc"
alias lalias="vim $HOME/.local_aliases"
alias vim="vim -p"

# move
alias vplug="cd $HOME/.vim/pack/dist/start"
alias tools="cd $HOME/tools"

